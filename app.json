
{
  "expo": {
    "name": "The Palace",
    "slug": "The Palace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cc422469-2a7f-4e9d-81b1-2c1fe8901ef5.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/7d34c77e-831d-4ee0-b02c-b042a09f6fa5.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#1A1A1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cc422469-2a7f-4e9d-81b1-2c1fe8901ef5.png",
        "backgroundColor": "#1A1A1A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively"
    },
    "web": {
      "favicon": "./assets/images/cc422469-2a7f-4e9d-81b1-2c1fe8901ef5.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow The Palace to access your contacts to autofill delivery addresses."
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": false
        }
      ]
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "The Palace"
}
